{
    "extends": "../tslint.json",
    "rules": {
        "no-string-literal": false,
        "object-literal-key-quotes": false,
        "prefer-for-of": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
